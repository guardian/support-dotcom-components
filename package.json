        "rollup-plugin-external-globals": "^0.13.0",
        "rollup-plugin-filesize": "^9.0.2",
        "rollup-plugin-peer-deps-external": "^2.2.3",
        "rollup-plugin-terser": "^7.0.2",
        "ts-jest": "^29.1.1",
        "ts-loader": "^9.2.5",
        "ts-node-dev": "^2.0.0",
        "tslib": "^2.5.3",
        "typescript": "~5.5.2",
        "typescript-json-schema": "^0.42.0",
        "webpack": "^5.94.0",
        "webpack-cli": "^4.7.2",
        "webpack-merge": "^5.8.0",
        "zod": "3.22.4"
    },
    "dependencies": {
        "@guardian/libs": "22.0.1",
        "aws-sdk": "^2.862.0",
        "compression": "1.7.4",
        "cors": "^2.8.5",
        "date-fns": "^2.25.0",
        "express": "^4.21.2",
        "jsonschema": "^1.4.0",
