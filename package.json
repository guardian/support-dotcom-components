{
  "name": "@sdc/mono",
  "version": "1.0.0",
  "description": "Service to serve Reader Revenue components to dotcom",
  "repository": "git@github.com:guardian/support-dotcom-components.git",
  "author": "The Guardian",
  "license": "MIT",
  "private": true,
  "scripts": {
    "server": "yarn workspace @sdc/server",
    "modules": "yarn workspace @sdc/modules",
    "shared": "yarn workspace @sdc/shared",
    "build": "tsc --build",
    "clean": "find ./packages -type d -name 'dist' -or -type f -name '*.tsbuildinfo' -exec rm -r {} +"
  },
  "workspaces": [
    "packages/*"
  ]
}
