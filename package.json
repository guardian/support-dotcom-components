{
    "name": "@guardian/support-dotcom-components",
    "version": "6.1.0",
    "description": "Service to serve Reader Revenue components to dotcom",
    "repository": "github:guardian/support-dotcom-components",
    "author": "The Guardian",
    "license": "MIT",
    "main": "dist/index.js",
    "module": "dist/index.esm.js",
    "types": "dist/dotcom/index.d.ts",
    "files": [
        "dist/**/*"
    ],
    "scripts": {
        "build-server": "webpack --config webpack.prod.js",
        "prestart": "webpack --config webpack.dev.js",
        "start-watch": "webpack --config webpack.dev.js --watch",
        "start-server": "stage=DEV PORT=8082 nodemon server-dist/server.js",
        "start": "concurrently --kill-others \"pnpm start-watch\" \"pnpm start-server\"",
        "build-dotcom": "rollup --config rollup.config.js",
        "build-dotcom-types": "tsc --project tsconfig.types.json",
        "publish-dry-run": "pnpm publish --dry-run",
        "publish": "pnpm publish",
        "lint": "eslint 'src/**/*.{ts,tsx}'",
        "test": "export stage=DEV; jest",
        "riffraff": "node-riffraff-artifact"
    },
    "devDependencies": {
        "@babel/core": "^7.26.10",
        "@babel/preset-typescript": "^7.26.0",
        "@changesets/cli": "^2.28.1",
        "@guardian/eslint-config-typescript": "^7.0.0",
        "@guardian/node-riffraff-artifact": "^0.3.2",
        "@guardian/prettier": "^5.0.0",
        "@rollup/plugin-alias": "^3.1.9",
        "@rollup/plugin-babel": "^5.3.1",
        "@rollup/plugin-commonjs": "^14.0.0",
        "@rollup/plugin-node-resolve": "^8.4.0",
        "@rollup/plugin-replace": "^2.4.2",
        "@types/body-parser": "^1.19.5",
        "@types/compression": "^1.7.5",
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.0",
        "@types/jest": "^24.9.1",
        "@types/lodash.debounce": "^4.0.9",
        "@types/node": "^22.13.10",
        "@types/seedrandom": "^3.0.8",
        "babel-loader": "^9.2.1",
        "body-parser": "^1.20.3",
        "concurrently": "^6.5.1",
        "eslint": "^8.57.1",
        "eslint-plugin-prettier": "5.2.3",
        "eslint-plugin-react": "^7.37.4",
        "fishery": "^0.4.1",
        "inquirer": "^7.3.3",
        "jest": "^29.7.0",
        "nodemon": "^3.1.9",
        "prettier": "^3.5.3",
        "rollup": "^2.79.2",
        "rollup-plugin-external-globals": "^0.13.0",
        "rollup-plugin-filesize": "^9.1.2",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "rollup-plugin-terser": "^7.0.2",
        "ts-jest": "^29.2.6",
        "ts-loader": "^9.5.2",
        "ts-node-dev": "^2.0.0",
        "tslib": "^2.8.1",
        "typescript": "~5.8.2",
        "typescript-json-schema": "^0.65.1",
        "webpack": "^5.98.0",
        "webpack-cli": "^4.10.0",
        "webpack-merge": "^5.10.0",
        "zod": "3.24.2"
    },
    "dependencies": {
        "@guardian/libs": "17.0.1",
        "aws-sdk": "^2.1692.0",
        "compression": "1.8.0",
        "cors": "^2.8.5",
        "date-fns": "^2.30.0",
        "express": "^4.21.2",
        "jsonschema": "^1.5.0",
        "lodash.debounce": "^4.0.8",
        "log4js": "^6.9.1",
        "seedrandom": "^3.0.5",
        "zod": "3.24.2"
    },
    "peerDependencies": {
        "@guardian/libs": "^17.0.0",
