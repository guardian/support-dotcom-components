        "@types/express": "^5.0.0",
        "@types/jest": "^24.0.25",
        "@types/lodash.debounce": "^4.0.6",
        "@types/node": "^22.10.7",
        "@types/seedrandom": "^3.0.1",
        "babel-loader": "^9.1.3",
        "body-parser": "^1.20.3",
        "concurrently": "^6.2.0",
        "eslint": "^9.19.0",
        "eslint-plugin-prettier": "5.1.3",
        "eslint-plugin-react": "^7.33.2",
        "fishery": "^0.3.0",
        "globals": "15.14.0",
        "inquirer": "^7.0.3",
        "jest": "^29.7.0",
        "nodemon": "^3.1.7",
        "prettier": "^3.0.0",
