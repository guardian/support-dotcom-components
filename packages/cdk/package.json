{
  "name": "@sdc/cdk",
  "version": "1.0.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "format": "prettier --write \"{lib,bin}/**/*.ts\"",
    "cdk": "cdk",
    "lint": "eslint lib/** bin/** --ext .ts --no-error-on-unmatched-pattern",
    "synth": "cdk synth --path-metadata false --version-reporting false"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.115.0",
    "@guardian/eslint-config-typescript": "^0.5.0",
    "@types/jest": "^26.0.20",
    "@types/node": "14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "aws-cdk": "1.115.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "~4.2.2"
  },
  "dependencies": {
    "@aws-cdk/aws-ec2": "1.115.0",
    "@aws-cdk/aws-events-targets": "1.115.0",
    "@aws-cdk/aws-iam": "1.115.0",
    "@aws-cdk/aws-lambda": "1.115.0",
    "@aws-cdk/aws-s3": "1.115.0",
    "@aws-cdk/core": "1.115.0",
    "@guardian/cdk": "23.5.1",
    "source-map-support": "^0.5.16"
  }
}
