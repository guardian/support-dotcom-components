  "compilerOptions": {
    "module": "ESNext",
    "target": "ES2018",
    "esModuleInterop": true,
    "moduleResolution": "bundler",
    "sourceMap": true,
    "strict": true,
    "noImplicitReturns": true,
    "jsx": "react-jsx",
